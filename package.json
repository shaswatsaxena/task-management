    "@types/supertest": "^2.0.9",
    "cross-env": "^7.0.2",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
